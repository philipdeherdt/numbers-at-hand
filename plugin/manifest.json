{
  "manifest_version": 2,

  "name": "__MSG_extensionname__",
  "description": "__MSG_extensiondescription__",
  "version": "1.5",
  "default_locale": "en",

  "browser_action": {
    "default_icon": {
      "16": "icons/Icon-16.png",
      "32": "icons/Icon-32.png",
      "48": "icons/Icon-48.png",
      "128": "icons/Icon-128.png"
    },
    "default_popup": "popup.html",
    "default_title": "__MSG_extensionhovertext__"
  },
  "permissions": ["storage", "contextMenus", "activeTab"],
  "background": {
    "scripts": ["js/common.js", "js/background.js"]
  },
  "icons": {
    "16": "icons/Icon-16.png",
    "32": "icons/Icon-32.png",
    "48": "icons/Icon-48.png",
    "128": "icons/Icon-128.png"
  },
  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com https://www.google-analytics.com https://www.googletagmanager.com https://code.jquery.com https://cdn.jsdelivr.net https://stackpath.bootstrapcdn.com; object-src 'self'"
}